type User {
    id : ID!,
    firstName : String,
    lastName : String,
    username: String,
    token : String,
    email : String!,
    roles : [Role],
    isActive : Boolean

}

type UserRevision {
    revisionDateTime : String,
    revisionNumber : Int,
    user : User

}

input CreateUserInput {
    email : String!
    password : String!

}